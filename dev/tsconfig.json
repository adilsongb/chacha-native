{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@adilsongb/chacha-native": [
        "../src/index"
      ],
      "@adilsongb/chacha-native/*": [
        "../src/*"
      ],
      "@/*": ["../src/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
