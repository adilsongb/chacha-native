{
  "name": "@adilsongb/chacha-native",
  "version": "0.1.4",
  "description": "A visual component library for building consistent and beautiful user interfaces in React Native apps.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsup",
    "dev": "tsup src/index.ts --watch",
    "dev:app": "cd dev && yarn start",
    "prepare": "tsup"
  },
  "keywords": [
    "react-native",
    "expo",
    "@adilsongb/chacha-native",
    "Chacha Native"
  ],
  "repository": "https://github.com/adilsongb/chacha-native",
  "bugs": {
    "url": "https://github.com/adilsongb/chacha-native/issues"
  },
  "author": "Adilson Gabriel <adilsongb.rabelo@gmail.com> (adilsongb)",
  "license": "MIT",
  "homepage": "https://github.com/adilsongb/chacha-native#readme",
  "dependencies": {
    "lottie-react-native": "^7.2.2"
  },
  "devDependencies": {
    "@types/react": "~19.0.0",
    "expo": "~53.0.0",
    "expo-module-scripts": "^4.1.6",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
